{
  "id": "builtin.language-basics-toml",
  "version": "0.0.0-dev",
  "name": "Language Basics Toml",
  "description": "Provides syntax highlighting and bracket matching in Toml files.",
  "languages": [
    {
      "id": "toml",
      "extensions": [
        ".toml",
        ".tml",
        ".replit"
      ],
      "fileNames": [
        "Cargo.lock",
        "Gopkg.lock"
      ],
      "tokenize": "src/tokenizeToml.js"
    }
  ],
  "repository": "https://github.com/lvce-editor/language-basics-toml"
}
